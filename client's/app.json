{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/ic_launcher.png",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "tumsa-f"
      }
    },
    "android": {
      "package": "com.tumsa.client",
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.tumsa.client",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to scan your face."
      }
    }
  }
}